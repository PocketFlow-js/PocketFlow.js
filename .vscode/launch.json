{
  "configurations": [
    {
      "type": "node-terminal",
      "name": "Run Script: essay",
      "request": "launch",
      "command": "pnpm run essay",
      "cwd": "${workspaceFolder}/packages/examples",
      "sourceMaps": true,
      "resolveSourceMapLocations": [
          "${workspaceFolder}/**",
          "!**/node_modules/**"
      ],
      "skipFiles": [
          "<node_internals>/**"
      ],
    }
  ]
}